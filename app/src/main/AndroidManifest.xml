<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.checkinuai_2"
    android:versionCode="1"
    android:versionName="1.0" >
    
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Inicio"
            android:label="" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name=".Firma"
            android:label="Firme y haga clic en CheckIn!" >
            <intent-filter>
                <action android:name="com.example.checkinuai_2.FIRMA" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity
            android:name=".Ajustes"
            android:label="Ajustes de la App" >
            <intent-filter>
                <action android:name="com.example.checkinuai_2.AJUSTES" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
    </application>

</manifest>